<h1>Listing runner_status_codes</h1>

<table>
  <thead>
    <tr>
      <th>Sequence</th>
      <th>Short code</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @runner_status_codes.each do |runner_status_code| %>
      <tr>
        <td><%= runner_status_code.sequence %></td>
        <td><%= runner_status_code.short_code %></td>
        <td><%= runner_status_code.description %></td>
        <td><%= link_to 'Show', runner_status_code %></td>
        <td><%= link_to 'Edit', edit_runner_status_code_path(runner_status_code) %></td>
        <td><%= link_to 'Destroy', runner_status_code, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br />

<%= link_to 'New Runner status code', new_runner_status_code_path %>
